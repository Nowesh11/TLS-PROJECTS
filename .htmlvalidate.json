{
  "extends": ["html-validate:recommended"],
  "rules": {
    "no-inline-style": "off",
    "no-trailing-whitespace": "off",
    "unique-landmark": "off",
    "no-implicit-button-type": "off",
    "text-content": "off",
    "no-raw-characters": "off",
    "input-attributes": "off",
    "form-dup-name": "off",
    "close-order": "error",
    "no-dup-id": "error",
    "empty-heading": "error"
  }
}